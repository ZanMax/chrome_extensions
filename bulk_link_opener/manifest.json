{
  "manifest_version": 3,
  "name": "URL OPENER",
  "description": "Open list of urls in new tab",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.min.js",
        "functions.js"
      ]
    }
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "permissions": [
    "tabs"
  ],
  "incognito": "split",
  "action": {
    "default_title": "Open url in new tab",
    "default_icon": "128.png",
    "default_popup": "index.html"
  }
}