{
  "manifest_version": 3,
  "name": "Copy WordPress Edit links",
  "description": "Copy WordPress Edit links",
  "version": "1.2",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "href.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "action": {
    "default_title": "Copy WordPress Edit links",
    "default_icon": "128.png",
    "default_popup": "popup.html"
  }
}