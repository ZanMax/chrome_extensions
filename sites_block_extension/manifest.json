{
  "manifest_version": 3,
  "name": "Sites Blocker Extension",
  "description": "This extension blocked sites that you need to optimize your time.",
  "version": "1.0",
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "action": {
    "default_title": "Sites Blocker Extension",
    "default_icon": "128.png",
    "default_popup": "popup.html"
  }
}